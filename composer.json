{
  "name": "anahkiasen/underscore-php",
  "description": "A redacted port of Underscore.js for PHP",
  "license": "MIT",
  "keywords": [
    "toolkit",
    "internals",
    "laravel"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "doctrine/inflector": "^1.0",
    "patchwork/utf8": "^1.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.11",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Underscore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Underscore\\": "tests"
    }
  },
  "scripts": {
    "test": ["php-cs-fixer fix --dry-run", "phpunit --verbose"],
    "lint": "php-cs-fixer fix"
  },
  "config": {
    "sort-packages": true
  }
}
